{
  "name": "LinkedInImageSearch",
  "manifest_version": 2,
  "version": "1.0",
  "description": "Chrome Extension for LinkedInImageSearch",
  "browser_action": {
    "default_icon": "icon3.png",
    "default_popup": "popup.html"
  },
  "icons": { "16": "icon3-16.png",
           "48": "icon3-48.png",
          "128": "icon3-128.png" },
  "background": {
      "scripts": ["background.js"]
  },
  "permissions": [
	  "notifications", "tabs", "contextMenus", "https://proj-nishantgill.c9.io/uploads"
	]
  }
}
